{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/transcript/(.*)",
        "dest": "/server.js"
      }
    ],
    "functions": {
      "api/*.js": {
        "memory": 512,
        "maxDuration": 10
      }
    }
}
